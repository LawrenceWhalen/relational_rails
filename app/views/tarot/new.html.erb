<%= form_with do |form| %>
  <%= form.label :printing_date, 'Printing Date:' %><br><br>
  <%= form.number_field :printing_date, in: 1.0..20.0, step: 0.01 %><br><br>
  <%= form.label :pre_owned, 'Pre-Owned?' %>
  <%= form.check_box :pre_owned, {}, 'true', 'false' %><br><br>
  <%= form.label :major_readings, 'Major Reading preformed:' %><br><br>+
  <%= form.text_area :major_readings, size: "70x5" %>
<% end %>
